  },
  "main": "detroit-main-menu.js",
  "module": "detroit-main-menu.es6.js",
  "umd": "detroit-main-menu.umd.js",
  "amd": "detroit-main-menu.amd.js",
  "scripts": {
    "test": "../../node_modules/.bin/wct --configFile ../../wct.conf.json node_modules/@cityofdetroit/detroit-main-menu/test/",
