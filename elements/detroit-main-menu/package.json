    "@webcomponents/webcomponentsjs": "2.2.7",
    "concurrently": "4.1.0",
    "gulp-babel": "8.0.0",
    "gulp-sourcemaps": "2.6.5",
    "lodash": "4.17.11",
    "polymer-build": "3.1.3",
